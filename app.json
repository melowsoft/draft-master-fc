{
  "expo": {
    "name": "DraftMaster FC",
    "slug": "draftmaster-fc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "draftmasterfc",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.draftmasterfc.app",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "draftmasterfc"
            ]
          },
          {
            "CFBundleURLSchemes": [
              "draftmasterfc"
            ]
          }
        ],
        "NSFaceIDUsageDescription": "Allow DraftMaster FC to use Face ID for secure authentication",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.draftmasterfc.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share lineups and chat images.",
          "cameraPermission": "The app accesses your camera to take photos for lineups."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "de468e1f-33cc-467b-84cf-ee4fc6704ca5"
      }
    },
    "owner": "melowsworld"
  }
}
